{
  "name": "gwop-checkout",
  "version": "0.1.0",
  "description": "Gwop Checkout SDK - Stripe-like payment primitives for self-hosted stores",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwop-app/gwop-checkout.git",
    "directory": "packages/gwop-checkout"
  },
  "homepage": "https://github.com/gwop-app/gwop-checkout/tree/main/packages/gwop-checkout#readme",
  "bugs": {
    "url": "https://github.com/gwop-app/gwop-checkout/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "gwop",
    "checkout",
    "payments",
    "invoices",
    "usdc",
    "solana",
    "ai",
    "agents",
    "commerce"
  ],
  "license": "MIT"
}
